{
  "name": "medpoisk-medical-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Медицинское приложение для поиска клиник и записи на прием",
  "keywords": [
    "medical",
    "healthcare",
    "clinic",
    "booking",
    "react"
  ],
  "author": "МедПоиск Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 3006",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "start": "npm run dev",
    "setup": "npm install && npm run format",
    "quality": "npm run lint && npm run format && npm run build",
    "type-check": "echo 'No TypeScript files to check'",
    "test": "echo 'No tests yet - ready for Jest/Vitest setup'"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "styled-components": "^6.1.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.56.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-performance": "^0.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.1.1",
    "vite": "^5.0.10",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
} 
